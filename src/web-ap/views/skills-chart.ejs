   function grafico_skills(dadosUsuarios){
let devsData = dadosUsuarios
      // Processa os dados para contar as habilidades √∫nicas
      const skillCounts = {};
      devsData.forEach(dev => {
          const uniqueSkills = new Set(dev.hardskill);
          uniqueSkills.forEach(skill => {
              skillCounts[skill] = (skillCounts[skill] || 0) + 1;
          });
      });

      const skillsData = Object.keys(skillCounts).map(skill => {
          return { skill: skill, count: skillCounts[skill] };
      });

      

      // Criando um objeto para armazenar os desenvolvedores por habilidade
      const devsBySkill = {};
      devsData.forEach(dev => {
          dev.hardskill.forEach(skill => {
              if (!devsBySkill[skill]) {
                  devsBySkill[skill] = new Set();
              }
              devsBySkill[skill].add(dev.id); // Mudan√ßa para usar 'id' que seria o username em vez de 'nomeCompleto'
          });
      });

      const colorScale = d3.scaleOrdinal(d3.schemeCategory10); // Escala de cores

      const svg = d3.select("svg"),
            width = +svg.attr("width"),
            height = +svg.attr("height");

      const pack = d3.pack()
                     .size([width, height])
                     .padding(1.5);

      const root = d3.hierarchy({children: skillsData})
                     .sum(d => d.count);

      const nodes = pack(root).leaves();

      const bubble = svg.selectAll("g")
                        .data(nodes)
                        .enter().append("g")
                        .attr("transform", d => `translate(${d.x},${d.y})`);

      bubble.append("circle")
            .attr("class", "bubble")
            .attr("r", d => d.r)
            .style("fill", d => colorScale(d.data.skill)); // Aplicar a cor aqu

            bubble.append("text")
            .attr("dy", ".3em")
            .style("text-anchor", "middle")
            .text(d => d.data.skill)
            .attr("font-family", "sans-serif")
            .attr("font-size", "13px") // Valor fixo para simplificar
            .attr("fill", "black");


            

            
      // Evento de mouseover na bolha
 bubble.on("click", function(event, d) {
  const skill = d.data.skill;
  const bubbleColor = colorScale(skill); // Armazenar a cor da bolha
  const bubbleColorWithTransparency =  `rgba(${d3.rgb(bubbleColor).r}, ${d3.rgb(bubbleColor).g}, ${d3.rgb(bubbleColor).b}, 0.7)`; // Adicionar transpar√™ncia √† cor da bolha
  const devs = Array.from(devsBySkill[skill]); // Convertendo o Set em Array
  const devsHtml = devs.map(username => {
      var namePart = username //.split(".")[0]; // Pegar apenas a primeira parte do username
     // namePart = namePart.charAt(0).toUpperCase() + namePart.slice(1); // Capitalizar a primeira letra
      return `<div style="text-align: left;">
                  <img src="https://felipeomc.github.io/teamplus/avatari.png" alt="User Icon" class="user-icon" style="width: 20px; height: 20px;">
                  <span style="color: white;">${namePart}</span>
              </div>`;
  }).join("")


  const popup = d3.select("#popup");
  popup.style("display", "block")
       .style("left", `${event.pageX + 10}px`)
       .style("top", `${event.pageY + 10}px`)
       .style("background-color", bubbleColorWithTransparency) // Definir a cor de fundo com transpar√™ncia
       .html(`<strong style="color: white; ">${skill}</strong><br><br>${devsHtml}`)
       .html(`<strong style="color: white; ">${skill}</strong><br><br>${devsHtml};
<button id="closePopup" style="position: absolute; top: 0; right: 0;">X</button>`);
});

      // Evento de mouseout para fechar o popup
     // Ap√≥s a defini√ß√£o do popup
document.addEventListener('click', function(event) {
  if (event.target.id === 'closePopup') {
    d3.select("#popup").style("display", "none");
  }
});





 
 


   }

