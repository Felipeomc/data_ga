  <script>
let dadosUsuarios
function MontarPerfilCard() {
    
      //$.getJSON("/users3", function(dadosUsuarios) {
        //grafico_skills(dadosUsuarios)
  
  $.getJSON("/dados-do-grafo", function(dadosDoGrafo) {
